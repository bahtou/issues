{
  "assumptions": {},
  "plugins": [
    ["@babel/plugin-transform-runtime", {
        "helpers": true,
        "corejs": 3,
        "version": "^7.14.7",
        "regenerator": true,
        "absoluteRuntime": false
    }],
    "@babel/plugin-syntax-dynamic-import"
  ],
  "presets": [
    ["@babel/preset-env", {
      "bugfixes": false,
      "spec": false,
      "loose": false,                      // see `assumptions`
      "modules": "auto",
      "debug": false,
      "include": [],
      "exclude": [],
      "useBuiltIns": false,          // see `plugin-transform-runtime`
      // "corejs": {
      //   "version": "3.15",
      //   "proposals": true
      // },
      "forceAllTransforms": false,
      "configPath": "../",
      "shippedProposals": false
    }],
    ["@babel/preset-react", {
      "runtime": "classic",
      "development": false,
      "throwIfNamespace": true,
      "pragma": "React.createElement",
      "pragmaFrag": "React.Fragment",
      "useBuiltIns": false,
      "useSpread": false
    }],
    ["@babel/preset-typescript", {
      "isTSX": true,
      "jsxPragma": "React",
      "jsxPragmaFrag": "React.Fragment",
      "allExtensions": true,
      "allowNamespaces": true,
      "allowDeclareFields": false,
      "onlyRemoveTypeImports": false
    }]
  ]
}
