{
  "jsc": {
    "externalHelpers": false,
    "loose": false,
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "transform": {
      "react": {
        "runtime": "automatic",
        "importSource": "react",
        "pragma": "React.createElement",
        "pragmaFrag": "React.Fragment",
        "throwIfNamespace": true,
        "useBuiltins": false,
        "refresh": false
      }
    },
    "keepClassNames": false,
    "baseUrl": "./src"
  },
  "minify": false,
  "module": {
    "type": "es6",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false,
    "ignoreDynamic": true
  },
  "sourceMaps": true,
  "inlineSourcesContent": true
}
